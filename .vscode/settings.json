{
  "editor.tabSize": 2,
  "editor.fontSize": 14,
  "editor.insertSpaces": true,
  "editor.mouseWheelZoom": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.eol": "\n",
  "eslint.validate": [
    "typescript",
    "javascript"
  ],
  "javascript.format.enable": false,
  "typescript.format.enable": false
}